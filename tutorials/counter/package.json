{
  "name": "sway-counter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "dist": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:check && npm run prettier:check",
    "lint:check": "eslint . --ext .ts,.tsx ",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "prettier:check": "prettier --check src",
    "prettier:format": "prettier --write src",
    "build": "sh ./scripts/build-contract.sh",
    "generate-private-key": "./node_modules/.bin/ts-node ./scripts/generate-private-key",
    "deploy": "./node_modules/.bin/ts-node ./scripts/deploy-contract.ts"
  },
  "dependencies": {
    "fuels": "0.12.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@fuel-ts/wallet": "0.12.0",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vitejs/plugin-react": "^1.3.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.2.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "prettier": "^2.7.1",
    "ts-node": "10.9.1",
    "typechain": "^8.0.0",
    "typechain-target-fuels": "0.12.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.7"
  }
}
