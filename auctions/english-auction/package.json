{
  "name": "english-auction",
  "version": "0.0.0",
  "author": "Fuel Labs <contact@fuel.sh> (https://fuel.network/)",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "homepage": "https://github.com/FuelLabs/sway-applications#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FuelLabs/sway-applications.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/FuelLabs/sway-applications/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "contracts": "pnpm exec english-auction-scripts run",
    "contracts:build": "pnpm exec english-auction-scripts build",
    "contracts:deploy": "pnpm exec english-auction-scripts deploy",
    "contracts:deploy-testnet": "NODE_ENV=testnet run-s contracts:deploy",
    "dev": "turbo run dev --parallel",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "pnpm lint:check --fix",
    "lint": "run-s lint:check prettier:check",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "scripts:setup": "pnpm run --filter=english-auction-scripts build && pnpm install",
    "services:clean": "make -C docker services-clean",
    "services:setup": "run-s scripts:setup services:run contracts",
    "services:run": "make -C docker services-run",
    "services:reset": "run-s services:clean services:setup"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@cypress/react": "^5.12.5",
    "@jest/types": "^28.1.3",
    "@swc/core": "^1.2.249",
    "@swc/jest": "^0.2.22",
    "@types/jest": "^28.1.8",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "cypress": "^10.7.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "english-auction-scripts": "workspace:0.1.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.3",
    "turbo": "^1.4.6",
    "typescript": "^4.8.3",
    "updates": "^13.1.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "react",
        "react-dom"
      ],
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    }
  }
}
