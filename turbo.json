{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": [".env", ".env.production", ".env.test"],
    "globalEnv": ["NODE_ENV"],
    "pipeline": {
      "ts:check": {
        "dependsOn": [],
        "outputs": ["./**/*.typegen.ts"]
      },
      "build": {
        "dependsOn": ["ts:check"],
        "outputs": ["dist/**"]
      },
      "build:all": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**"]
      },
      "build:preview": {
        "dependsOn": ["^build"],
        "outputs": ["./dist/"]
      }
    }
  }
  